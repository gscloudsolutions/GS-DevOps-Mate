<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Commit_SHA__c</fullName>
        <description>The commit SHA for a module. This represents up to what commit, the source is being deployed from a repo to this Org.</description>
        <externalId>false</externalId>
        <label>Commit SHA</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Git_Tag__c</fullName>
        <description>The Git commit tag for a module. This represents up to what commit, the source is being deployed from a repo to this Org.</description>
        <externalId>false</externalId>
        <label>Git Tag</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Module_Name__c</fullName>
        <defaultValue>&quot;All&quot;</defaultValue>
        <description>The name of the module deployed in case of a SFDX repo. For a Non-SFDX repo, it would be All by default.</description>
        <externalId>false</externalId>
        <label>Module Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Deployment_Id__c</fullName>
        <description>The Id of the latest successful deployment.</description>
        <externalId>false</externalId>
        <label>Deployment Id</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Deployment Info</label>
    <visibility>Public</visibility>
</CustomObject>
